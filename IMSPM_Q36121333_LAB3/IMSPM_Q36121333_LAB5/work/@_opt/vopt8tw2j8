library verilog;
use verilog.vl_types.all;
entity encoder is
    port(
        clk             : in     vl_logic;
        rst             : in     vl_logic;
        datain          : in     vl_logic_vector(7 downto 0);
        en              : out    vl_logic_vector(3 downto 0)
    );
end encoder;
